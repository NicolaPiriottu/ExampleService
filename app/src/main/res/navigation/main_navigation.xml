<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_navigation"
    app:startDestination="@id/speedFragment">

    <fragment
        android:id="@+id/speedFragment"
        android:name="com.example.exampleservice.SpeedFragment"
        android:label="fragment_speed"
        tools:layout="@layout/fragment_speed">
        <action
            android:id="@+id/to_noGpsFragment"
            app:destination="@id/noGpsFragment" />
    </fragment>
    <fragment
        android:id="@+id/noGpsFragment"
        android:name="com.example.exampleservice.NoGpsFragment"
        android:label="fragment_no_gps"
        tools:layout="@layout/fragment_no_gps" />
</navigation>